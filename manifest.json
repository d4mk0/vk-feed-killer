{
  "manifest_version": 2,
  "name": "Убийца ленты ВКонтакте",
  "description": "Расширение позволяет скрыть новостную ленту ВКонтакте, а вместо неё увидеть что-то более мотивирующее",
  "version": "0.0.1",
  "permissions": ["http://vk.com/*", "https://vk.com/*"],
  "content_scripts": [
    {
      "matches": ["http://vk.com/*", "https://vk.com/*"],
      "js": ["jquery-2.1.4.min.js", "runner.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}